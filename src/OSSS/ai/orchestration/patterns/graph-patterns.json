{
  "patterns": {
    "standard": {
      "description": "Standard contract: refiner -> final -> END.",
      "entry_point": "refiner",
      "edges": [
        { "from": "refiner", "to": "final" },
        { "from": "final", "to": "END" }
      ]
    },
    "data_query": {
      "description": "Superset-capable contract (Option A): default refiner -> final. Refiner may route to data_query. data_query -> (historian | final | END). historian -> final -> END.",
      "entry_point": "refiner",
      "conditional_edges": {
        "refiner": "refiner_route_query_or_end",
        "data_query": "route_after_data_query"
      },
      "conditional_destinations": {
        "refiner": {
          "data_query": "data_query",
          "final": "final"
        },
        "data_query": {
          "historian": "historian",
          "final": "final",
          "END": "END"
        }
      },
      "edges": [
        { "from": "historian", "to": "final" },
        { "from": "final", "to": "END" }
      ]
    }
  }
}
