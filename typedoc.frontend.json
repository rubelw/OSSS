{
  "$schema": "https://typedoc.org/schema.json",
  "tsconfig": "src/osss-web/tsconfig.json",
  "entryPointStrategy": "expand",
  "entryPoints": [
    "src/osss-web/lib",
    "src/osss-web/components",
    "src/osss-web/types"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "out": "docs/api/web",
  "exclude": [
    "src/osss-web/app/**",
    "**/.next/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/*.test.*",
    "**/*.spec.*",
    "**/__tests__/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true
}
