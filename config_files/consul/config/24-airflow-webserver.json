{
  "service": {
    "name": "airflow-webserver",
    "id": "airflow-webserver-8080",
    "address": "airflow-webserver",
    "port": 8080,
    "tags": ["profile:airflow","http"],
    "checks": [
      {
        "name": "airflow-http",
        "http": "http://airflow-webserver:8080/health",
        "interval": "15s",
        "timeout": "5s"
      }
    ]
  }
}
