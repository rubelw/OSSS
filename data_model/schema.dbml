}

Table work_order_parts {
  work_order_id CHAR(36) [not null, increment]
  part_id CHAR(36) [increment]
  qty numeric(12,2) [not null, default: "1", increment]
  unit_cost numeric(12,2) [increment]
  extended_cost numeric(12,2) [increment]
  notes text [increment]
  created_at timestamp [not null, default: "now()", increment]
  updated_at timestamp [not null, default: "now()", increment]
  id CHAR(36) [pk, not null, default: "gen_random_uuid()", increment]
}

Table work_order_tasks {
  work_order_id CHAR(36) [not null, increment]
  seq int [not null, default: "1", increment]
  title varchar(255) [not null, increment]
  is_mandatory boolean [not null, default: "false", increment]
  status varchar(32) [increment]
  completed_at timestamp [increment]
  notes text [increment]
  created_at timestamp [not null, default: "now()", increment]
  updated_at timestamp [not null, default: "now()", increment]
  id CHAR(36) [pk, not null, default: "gen_random_uuid()", increment]
}

Table work_order_time_logs {
  work_order_id CHAR(36) [not null, increment]
  user_id CHAR(36) [increment]
  started_at timestamp [increment]
  ended_at timestamp [increment]
  hours numeric(10,2) [increment]
  hourly_rate numeric(12,2) [increment]
  cost numeric(12,2) [increment]
  notes text [increment]
  created_at timestamp [not null, default: "now()", increment]
  updated_at timestamp [not null, default: "now()", increment]
  id CHAR(36) [pk, not null, default: "gen_random_uuid()", increment]
}

Table work_orders {
  school_id CHAR(36) [increment]
  building_id CHAR(36) [increment]
  space_id CHAR(36) [increment]
  asset_id CHAR(36) [increment]
  request_id CHAR(36) [unique, increment]
  status varchar(32) [not null, default: "'open'", increment]
  priority varchar(16) [increment]
  category varchar(64) [increment]
  summary varchar(255) [not null, increment]
  description text [increment]
  requested_due_at timestamp [increment]
  scheduled_start_at timestamp [increment]
  scheduled_end_at timestamp [increment]
  completed_at timestamp [increment]
  assigned_to_user_id CHAR(36) [increment]
  materials_cost numeric(12,2) [increment]
  labor_cost numeric(12,2) [increment]
  other_cost numeric(12,2) [increment]
  attributes JSON [increment]
  created_at timestamp [not null, default: "now()", increment]
  updated_at timestamp [not null, default: "now()", increment]
  id CHAR(36) [pk, not null, default: "gen_random_uuid()", increment]
}



